{
  "name": "@koromerzhin/labstag",
  "version": "1.4.0",
  "description": "Site Internet sous Symfony",
  "main": "index.js",
  "scripts": {
    "assets": "npm run symfony assets:install public --symlink --relative",
    "bash:phpfpm": "docker exec -ti $(npx korojscommands docker_getname-container --container phpfpm) bash",
    "bdd:mariadb": "korojscommands bddset-mariadb",
    "cmd:install:all": "npm-run-all exec doctrine:migrate assets encore:dev cmd:lint:all",
    "cmd:install:dev": "npm-run-all cmd:install:all download:phar doctrine:fixtures cmd:labstag:all",
    "cmd:install:prod": "npm-run-all cmd:install:all doctrine:fixtures cmd:labstag:all encore:build",
    "cmd:labstag:all": "npm-run-all labstag:install labstag:guard-route labstag:workflows-show",
    "cmd:php-cs-fixer_phpcbf": "npm-run-all lint:php-cs-fixer lint:phpcbf",
    "cmd:validate_outdated": "npm-run-all composer:validate composer:outdated",
    "composer:fund": "npm run composer fund",
    "composer:install:dev": "npm run composer install --no-progress --prefer-dist --optimize-autoloader",
    "composer:install:prod": "npm run composer install --no-progress --prefer-dist --optimize-autoloader --no-dev",
    "composer:install": "npm run composer install",
    "composer:outdated": "npm run composer outdated",
    "composer:suggests": "npm run composer suggests --by-suggestion",
    "composer:update": "npm run composer update",
    "composer:validate": "npm run composer validate",
    "composer": "docker exec -i $(npx korojscommands docker_getname-container --container phpfpm) symfony composer",
    "contributors:add": "all-contributors add",
    "contributors:check": "all-contributors check",
    "contributors:generate": "all-contributors generate",
    "docker:deploy": "korojscommands docker_deploy",
    "docker:getpull-image": "korojscommands docker_getpull-image",
    "docker:ls": "korojscommands docker_ls",
    "docker:waiting": "korojscommands docker_waiting --status running --container apache phpfpm",
    "doctrine:fixtures": "npm run symfony doctrine:fixtures:load -n",
    "doctrine:migrate": "npm run symfony doctrine:migrations:migrate -n",
    "doctrine:schema:update": "npm run symfony doctrine:schema:update --force",
    "doctrine:validate": "npm run symfony doctrine:schema:validate",
    "download:phar": "korojscommands php_download-phar",
    "encore:build": "encore production --progress",
    "encore:dev:server": "encore dev-server",
    "encore:dev": "encore dev",
    "encore:watch": "encore dev --watch",
    "eslint:fix": "eslint assets/js/ --fix",
    "eslint": "eslint assets/js/",
    "exec": "npm-run-all bdd:mariadb docker:getpull-image docker:deploy docker:waiting docker:ls",
    "jscpd:report": "jscpd assets apps/src -r html -s",
    "jscpd": "jscpd assets apps/src",
    "labstag:geocode:install": "npm run symfony labstag:geocode:install",
    "labstag:guard-route": "npm run symfony labstag:guard-route",
    "labstag:install": "npm run symfony labstag:install",
    "labstag:workflows-show": "npm run symfony labstag:workflows-show",
    "lint:all": "npm-run-all validate_outdated php-cs-fixer_phpcbf eslint:fix lint:stylelint:fix lint:twig lint:container lint:yaml lint:phpaudit lint:markdown rector",
    "lint:container": "npm run symfony lint:container",
    "lint:markdown": "markdownlint README.md",
    "lint:php-cs-fixer": "npm run php php-cs-fixer.phar fix src",
    "lint:phpaudit": "npm-run-all lint:phpcs lint:phpmd lint:phpmnd lint:phpstan",
    "lint:phpcbf": "npm run php phpcbf.phar -d memory_limit=-1 --report=diff -p --extensions=php --standard=phpcs.xml",
    "lint:phpcs:error": "npm run php phpcs.phar  --report=full --extensions=php --warning-severity=0 --standard=phpcs.xml",
    "lint:phpcs:warning": "npm run php phpcs.phar --report=full --extensions=php --error-severity=0 --standard=phpcs.xml",
    "lint:phpcs": "npm run php phpcs.phar --report=full --extensions=php src --standard=phpcs.xml",
    "lint:phpdoc": "npm run php phpDocumentor.phar -d src -t public/docs",
    "lint:phploc": "npm run php phploc.phar src",
    "lint:phpmd": "npm run php -d error_reporting=24575 phpmd.phar src,features/bootstrap ansi phpmd.xml",
    "lint:phpmnd": "npm run composer run phpmnd",
    "lint:phpstan": "npm run php phpstan.phar analyse src",
    "lint:stylelint:fix": "stylelint assets/scss/*.scss --fix",
    "lint:stylelint": "stylelint assets/scss/*.scss",
    "lint:twig": "npm run symfony lint:twig templates",
    "lint:yaml": "npm run symfony lint:yaml config translations --parse-tags",
    "messenger:consume": "npm run symfony messenger:consume async -vv",
    "php": "docker exec -i $(npx korojscommands docker_getname-container --container phpfpm) php -d memory_limit=-1",
    "rector:dry": "npm run composer run rector-dry",
    "rector": "npm run composer run rector",
    "symfony": "docker exec -i $(npx korojscommands docker_getname-container --container phpfpm) symfony console",
    "test:behat": "npm run php behat.phat --config behat.yaml",
    "test:phpunit:unit-integration": "npm run composer run simple-phpunit-unit-integration",
    "test:phpunit": "npm run composer run simple-phpunit",
    "translation:update": "npm run symfony translation:update --force --format=yaml --clean fr",
    "workflow:dump": "npm run symfony workflow:dump"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koromerzhin/labstag.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/koromerzhin/labstag/issues"
  },
  "homepage": "https://github.com/koromerzhin/labstag#readme",
  "devDependencies": {
    "@symfony/stimulus-bridge": "2.0.2",
    "@symfony/webpack-encore": "1.6.1",
    "core-js": "3.21.0",
    "cypress": "9.4.1",
    "eslint": "^7.12.1",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "file-loader": "6.2.0",
    "postcss-loader": "^7.0.1",
    "regenerator-runtime": "0.13.9",
    "sass": "1.49.4",
    "sass-loader": "12.4.0",
    "stimulus": "2.0.0",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "url-loader": "4.1.1",
    "webpack-notifier": "1.15.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.2.1",
    "@popperjs/core": "2.11.6",
    "bootstrap": "5.2.1",
    "ckeditor4": "^4.18.0",
    "flag-icon-css": "^4.0.0",
    "jquery": "3.6.0",
    "korojscommands": "^1.2.7",
    "sortablejs": "1.14.0",
    "tarteaucitronjs": "^1.9.5",
    "tom-select": "1.7.8"
  }
}
