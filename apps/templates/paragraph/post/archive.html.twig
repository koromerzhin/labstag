<section id="{{ paragraph_id(paragraph) }}" class="{{ paragraph_class(paragraph) }}">
    <ol>
        {% for archive in archives %}
            <li>
                <a href="{{ path('front_article_year', {'year': archive.code}) }}">{{ archive.published|format_datetime(pattern="MMMM Y") }}</a>
            </li>
        {% endfor %}
    </ol>
</section>