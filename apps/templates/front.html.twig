{% extends 'base.html.twig' %}
{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('front') }}
{% endblock %}
{% block content %}
    {% for row, blockhtml in blocks %}
        <{{ row }}>
            {% for html in blockhtml %}
                {{ debug_begin(html.template) }}
                {{ html.data.content|raw }}
                {{ debug_end(html.template) }}
            {% endfor %}
        </{{ row }}>
    {% endfor %}
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('front') }}
{% endblock %}
