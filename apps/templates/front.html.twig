{% extends 'base.html.twig' %}
{% import "macro.html.twig" as macro_site %}
{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('front') }}
{% endblock %}
{% block content %}
    {{ macro_site.flashbag(app) }}
    {% for row, block in blocks %}
        <{{ row }}>
            {% for html in block %}
                {{ html.content|raw }}
            {% endfor %}
        </{{ row }}>
    {% endfor %}
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('front') }}
{% endblock %}
