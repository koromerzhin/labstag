{% extends 'admin.html.twig' %}
{% block body %}
    {{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.published) }}
    {{ form_row(form.slug) }}
    {{ form_row(form.summary) }}
    {{ form_row(form.paragraph) }}
    {{ form_row(form.refuser) }}
    {{ form_row(form.metas) }}
    {{ form_end(form) }}
{% endblock %}
{% block modal %}
    {{ parent() }}
    {% include "admin/paragraph/modal.html.twig" with {'paragraphurl': 'admin_history_paragraph_index' } %}
{% endblock %}
