<form method="post" action="{{ path('adresse_user_delete', {'id': adresse_user.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ adresse_user.id) }}">
    <button class="btn">Delete</button>
</form>
