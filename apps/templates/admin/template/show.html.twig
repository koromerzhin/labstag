{% extends 'admin/template/layout.html.twig' %}
{% block body %}
    <table>
        <tbody>
            <tr>
                <th>{{ "Name"|trans }}</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>{{ "Code"|trans }}</th>
                <td>{{ entity.code }}</td>
            </tr>
            <tr>
                <th>{{ "Html"|trans }}</th>
                <td>{{ entity.html|raw }}</td>
            </tr>
            <tr>
                <th>{{ "Text"|trans }}</th>
                <td>{{ entity.text }}</td>
            </tr>
        </tbody>
    </table>
{% endblock %}
