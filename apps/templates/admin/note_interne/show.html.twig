{% extends 'admin/note_interne/layout.html.twig' %}
{% block body %}
    <table
  data-toggle="table"
  data-search="true"
  data-show-columns="true">
        <tbody>
            <tr>
                <th>Title</th>
                <td>{{ note_interne.title }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ note_interne.content|raw }}</td>
            </tr>
            <tr>
                <th>Enable</th>
                <td>{{ note_interne.enable ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Date_debut</th>
                <td>{{ note_interne.dateDebut ? note_interne.dateDebut|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Date_fin</th>
                <td>{{ note_interne.dateFin ? note_interne.dateFin|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>User</th>
                <td>{{ note_interne.refuser }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_noteinterne_index') }}">back to list</a>

    <a href="{{ path('admin_noteinterne_edit', {'id': note_interne.id}) }}">edit</a>

    {{ include('admin/note_interne/_delete_form.html.twig') }}
{% endblock %}
