{
    "type": "project",
    "name": "koromerzhin/labstag",
    "description": "Site Internet sous Symfony",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "ext-amqp": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "adam-paterson/oauth2-slack": "1.1.*",
        "api-platform/core": "3.1.*",
        "beberlei/doctrineextensions": "1.3.*",
        "doctrine/annotations": "2.0.*",
        "doctrine/doctrine-bundle": "2.10.*",
        "doctrine/doctrine-migrations-bundle": "3.2.*",
        "doctrine/orm": "2.15.*",
        "embed/embed": "4.4.*",
        "fakerphp/faker": "1.23.*",
        "gedmo/doctrine-extensions": "3.11.*",
        "giggsey/libphonenumber-for-php": "8.13.*",
        "imagine/imagine": "1.3.*",
        "knplabs/knp-menu-bundle": "3.2.*",
        "knplabs/knp-paginator-bundle": "6.2.*",
        "league/oauth2-client": "2.7.*",
        "league/oauth2-facebook": "2.2.*",
        "league/oauth2-github": "3.1.*",
        "league/oauth2-google": "4.0.*",
        "league/oauth2-instagram": "3.1.*",
        "league/oauth2-linkedin": "5.1.*",
        "liip/imagine-bundle": "2.11.*",
        "michaelkaefer/oauth2-amazon": "1.0.*",
        "mmo/faker-images": "0.8.*",
        "nelmio/cors-bundle": "2.3.*",
        "omines/oauth2-gitlab": "3.5.*",
        "phpdocumentor/reflection-docblock": "5.3.*",
        "rtheunissen/oauth2-reddit": "2.0.*",
        "spipu/html2pdf": "5.2.*",
        "stevenmaguire/oauth2-bitbucket": "3.0.*",
        "stevenmaguire/oauth2-dropbox": "3.1.*",
        "stevenmaguire/oauth2-paypal": "2.0.*",
        "symfony/apache-pack": "1.0.*",
        "symfony/asset": "6.3.*",
        "symfony/cache": "6.3.*",
        "symfony/console": "6.3.*",
        "symfony/doctrine-messenger": "6.3.*",
        "symfony/dotenv": "6.3.*",
        "symfony/event-dispatcher": "6.3.*",
        "symfony/expression-language": "6.3.*",
        "symfony/finder": "6.3.*",
        "symfony/flex": "2.3.*",
        "symfony/form": "6.3.*",
        "symfony/framework-bundle": "6.3.*",
        "symfony/http-client": "6.3.*",
        "symfony/intl": "6.3.*",
        "symfony/ldap": "6.3.*",
        "symfony/lock": "6.3.*",
        "symfony/mailer": "6.3.*",
        "symfony/mercure": "0.6.*",
        "symfony/messenger": "6.3.*",
        "symfony/mime": "6.3.*",
        "symfony/monolog-bundle": "3.8.*",
        "symfony/notifier": "6.3.*",
        "symfony/process": "6.3.*",
        "symfony/property-access": "6.3.*",
        "symfony/property-info": "6.3.*",
        "symfony/proxy-manager-bridge": "6.3.*",
        "symfony/rate-limiter": "6.3.*",
        "symfony/runtime": "6.3.*",
        "symfony/security-bundle": "6.3.*",
        "symfony/serializer": "6.3.*",
        "symfony/string": "6.3.*",
        "symfony/translation": "6.3.*",
        "symfony/twig-bundle": "6.3.*",
        "symfony/uid": "6.3.*",
        "symfony/validator": "6.3.*",
        "symfony/web-link": "6.3.*",
        "symfony/webapp-meta": "1.0.*",
        "symfony/webpack-encore-bundle": "2.0.*",
        "symfony/workflow": "6.3.*",
        "symfony/yaml": "6.3.*",
        "twig/cssinliner-extra": "3.6.*",
        "twig/extra-bundle": "3.6.*",
        "twig/intl-extra": "3.6.*",
        "vantezzen/oauth2-pinterest": "2.2.*",
        "vertisan/oauth2-twitch-helix": "1.1.*",
        "webonyx/graphql-php": "15.5.*",
        "wohali/oauth2-discord-new": "1.2.*"
    },
    "require-dev": {
        "behat/behat": "3.13.*",
        "doctrine/doctrine-fixtures-bundle": "3.4.*",
        "phpmd/phpmd": "2.13.*",
        "phpstan/phpdoc-parser": "1.22.*",
        "phpstan/phpstan": "1.10.*",
        "phpstan/phpstan-deprecation-rules": "1.1.*",
        "phpstan/phpstan-doctrine": "1.3.*",
        "phpstan/phpstan-mockery": "1.1.*",
        "phpstan/phpstan-phpunit": "1.3.*",
        "phpstan/phpstan-symfony": "1.3.*",
        "phpunit/phpunit": "10.2.*",
        "povils/phpmnd": "3.1.*",
        "rector/phpstan-rules": "0.6.*",
        "rector/rector": "0.17.*",
        "squizlabs/php_codesniffer": "3.7.*",
        "symfony/browser-kit": "6.3.*",
        "symfony/css-selector": "6.3.*",
        "symfony/debug-bundle": "6.3.*",
        "symfony/maker-bundle": "1.49.*",
        "symfony/phpunit-bridge": "6.3.*",
        "symfony/stopwatch": "6.3.*",
        "symfony/web-profiler-bundle": "6.3.*"
    },
    "config": {
      "bin-dir": "bin/",
      "optimize-autoloader": true,
      "preferred-install": {
        "*": "dist"
      },
      "platform": {
        "php": "8.2"
      },
      "sort-packages": true,
      "allow-plugins": {
        "composer/package-versions-deprecated": true,
        "symfony/flex": true,
        "symfony/runtime": true
      }
    },
    "autoload": {
      "psr-4": {
        "Labstag\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Labstag\\Tests\\": "tests/"
      }
    },
    "replace": {
      "paragonie/random_compat": "2.*",
      "symfony/polyfill-ctype": "*",
      "symfony/polyfill-iconv": "*",
      "symfony/polyfill-php72": "*"
    },
    "scripts": {
      "behat": "behat --config behat.yaml",
      "php-cs-fixer": "php php-cs-fixer.phar fix src",
      "php-cs-fixer:dry": "php php-cs-fixer.phar fix src --dry-run -v --diff",
      "phpcbf": "phpcbf --report=diff --extensions=php --standard=phpcs.xml",
      "phpcs": "phpcs --report=full --extensions=php src --standard=phpcs.xml",
      "phpcs:error": "phpcs  --report=full --extensions=php --warning-severity=0 --standard=phpcs.xml",
      "phpcs:warning": "phpcs --report=full --extensions=php --error-severity=0 --standard=phpcs.xml",
      "phpdoc": "php phpDocumentor.phar -d src -t public/docs",
      "phploc": "php phploc.phar src",
      "phpmd": "phpmd -e error_reporting=24575 src,features/bootstrap ansi phpmd.xml",
      "phpmnd": "phpmnd src features/bootstrap tests",
      "rector": "rector process --clear-cache",
      "rector-dry": "rector process --clear-cache --dry-run",
      "simple-phpunit": "simple-phpunit --verbose",
      "simple-phpunit-unit-integration": "simple-phpunit --testsuite unit,integration --verbose",
      "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
      "post-install-cmd": [
        "@auto-scripts"
      ],
      "post-update-cmd": [
        "@auto-scripts"
      ]
    },
    "conflict": {
      "symfony/symfony": "*"
    },
    "extra": {
      "symfony": {
            "allow-contrib": true,
            "require": "6.3.*",
            "docker": false
        }
    }
  }
