    "vertisan/oauth2-twitch-helix": "1.1.1",
    "webonyx/graphql-php": "14.11.3",
    "wohali/oauth2-discord-new": "1.1.0"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "3.4.1",
    "povils/phpmnd": "2.4.0",
    "symfony/browser-kit": "5.4.2",
    "symfony/css-selector": "5.4.2",
    "symfony/debug-bundle": "5.4.2",
    "symfony/maker-bundle": "1.36.4",
    "symfony/phpunit-bridge": "5.4.0",
    "symfony/stopwatch": "5.4.0",
    "symfony/var-dumper": "5.4.2",
    "symfony/web-profiler-bundle": "5.4.2"
  },
  "config": {
    "optimize-autoloader": true,
    "bin-dir": "bin",
    "preferred-install": {
      "*": "dist"
    },
    "platform": {
      "php": "8.0.9"
    },
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "Labstag\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Labstag\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "phpmnd": "phpmnd src features/bootstrap tests",
    "simple-phpunit": "simple-phpunit --verbose",
    "simple-phpunit-unit-integration": "simple-phpunit --testsuite unit,integration --verbose",
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "ckeditor:install": "symfony-cmd",
      "elfinder:install": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": "true",
      "require": "5.3.*"
    }
  }
}
