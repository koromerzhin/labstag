{
    "type": "project",
    "name": "koromerzhin/labstag",
    "description": "Site Internet sous Symfony",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "ext-amqp": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "adam-paterson/oauth2-slack": "^1.1",
        "api-platform/core": "2.6.8",
        "beberlei/doctrineextensions": "1.3.0",
        "bluemmb/faker-picsum-photos-provider": "2.0",
        "doctrine/annotations": "1.13.2",
        "doctrine/doctrine-bundle": "2.5.5",
        "doctrine/doctrine-migrations-bundle": "3.2.1",
        "doctrine/orm": "2.10.2",
        "fakerphp/faker": "v1.18.0",
        "gedmo/doctrine-extensions": "3.5.0",
        "giggsey/libphonenumber-for-php": "8.12.41",
        "helios-ag/fm-elfinder-bundle": "12.1.2",
        "imagine/imagine": "1.2.4",
        "knplabs/knp-menu-bundle": "3.2.0",
        "knplabs/knp-paginator-bundle": "5.8.0",
        "league/oauth2-client": "^2.6",
        "league/oauth2-facebook": "^2.0",
        "league/oauth2-github": "^3.0",
        "league/oauth2-google": "^4.0",
        "league/oauth2-instagram": "^3.0",
        "league/oauth2-linkedin": "^5.1",
        "liip/imagine-bundle": "2.7.6",
        "michaelkaefer/oauth2-amazon": "^1.0",
        "omines/oauth2-gitlab": "^3.4",
        "phpdocumentor/reflection-docblock": "5.3.0",
        "phpstan/phpdoc-parser": "^1.2",
        "rtheunissen/oauth2-reddit": "^2.0",
        "sensio/framework-extra-bundle": "6.2.1",
        "spipu/html2pdf": "5.2.4",
        "stevenmaguire/oauth2-bitbucket": "^3.0",
        "stevenmaguire/oauth2-dropbox": "^3.1",
        "stevenmaguire/oauth2-paypal": "^2.0",
        "symfony/apache-pack": "1.0.1",
        "symfony/asset": "5.4.0",
        "symfony/cache": "5.4.2",
        "symfony/console": "5.4.2",
        "symfony/dotenv": "5.4.2",
        "symfony/event-dispatcher": "5.4.0",
        "symfony/expression-language": "5.4.0",
        "symfony/finder": "5.4.2",
        "symfony/flex": "1.17.6",
        "symfony/form": "5.4.2",
        "symfony/framework-bundle": "5.4.2",
        "symfony/http-client": "5.4.2",
        "symfony/intl": "5.4.2",
        "symfony/ldap": "5.4.2",
        "symfony/lock": "5.4.2",
        "symfony/mailer": "5.4.2",
        "symfony/mercure": "0.6.1",
        "symfony/messenger": "5.4.2",
        "symfony/mime": "5.4.2",
        "symfony/monolog-bundle": "3.7.1",
        "symfony/notifier": "5.4.2",
        "symfony/process": "5.4.2",
        "symfony/property-access": "5.4.2",
        "symfony/property-info": "5.4.2",
        "symfony/proxy-manager-bridge": "5.4.2",
        "symfony/rate-limiter": "5.4.2",
        "symfony/runtime": "5.4.1",
        "symfony/security-bundle": "5.4.2",
        "symfony/serializer": "5.4.2",
        "symfony/string": "5.4.2",
        "symfony/translation": "5.4.2",
        "symfony/twig-bundle": "5.4.0",
        "symfony/uid": "5.4.*",
        "symfony/validator": "5.4.2",
        "symfony/web-link": "5.4.0",
        "symfony/webapp-meta": "^1.0",
        "symfony/webpack-encore-bundle": "1.13.2",
        "symfony/workflow": "5.4.2",
        "symfony/yaml": "5.4.2",
        "twig/cssinliner-extra": "3.3.5",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "3.3.5",
        "twig/twig": "^2.12|^3.0",
        "vantezzen/oauth2-pinterest": "^2.2",
        "vertisan/oauth2-twitch-helix": "^1.1",
        "webonyx/graphql-php": "14.11.5",
        "wohali/oauth2-discord-new": "^1.1"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "3.4.1",
        "phpunit/phpunit": "9.5.13",
        "povils/phpmnd": "2.5.0",
        "rector/rector": "^0.12.12",
        "symfony/browser-kit": "5.4.2",
        "symfony/css-selector": "5.4.2",
        "symfony/debug-bundle": "5.4.2",
        "symfony/maker-bundle": "1.36.4",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "5.4.0",
        "symfony/web-profiler-bundle": "5.4.2"
    },
    "config": {
      "bin-dir": "bin/",
      "optimize-autoloader": true,
      "preferred-install": {
        "*": "dist"
      },
      "platform": {
        "php": "8.0.9"
      },
      "sort-packages": true,
      "allow-plugins": {
        "composer/package-versions-deprecated": true,
        "symfony/flex": true,
        "symfony/runtime": true
      }
    },
    "autoload": {
      "psr-4": {
        "Labstag\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Labstag\\Tests\\": "tests/"
      }
    },
    "replace": {
      "paragonie/random_compat": "2.*",
      "symfony/polyfill-ctype": "*",
      "symfony/polyfill-iconv": "*",
      "symfony/polyfill-php72": "*"
    },
    "scripts": {
      "phpmnd": "phpmnd src features/bootstrap tests",
      "simple-phpunit": "simple-phpunit --verbose",
      "simple-phpunit-unit-integration": "simple-phpunit --testsuite unit,integration --verbose",
      "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "elfinder:install": "symfony-cmd"
        },
      "post-install-cmd": [
        "@auto-scripts"
      ],
      "post-update-cmd": [
        "@auto-scripts"
      ]
    },
    "conflict": {
      "symfony/symfony": "*"
    },
    "extra": {
      "symfony": {
        "allow-contrib": "true",
        "require": "5.4.*"
      }
    }
  }
