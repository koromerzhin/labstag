    "setasign/fpdf": "1.8.4",
    "setasign/fpdi": "2.3.6",
    "spipu/html2pdf": "5.2.3",
    "stevenmaguire/oauth2-bitbucket": "3.0.0",
    "stevenmaguire/oauth2-dropbox": "3.1.0",
