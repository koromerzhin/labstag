{
    "type": "project",
    "name": "koromerzhin/labstag",
    "description": "Site Internet sous Symfony",
    "license": "MIT",
    "require": {
        "php": ">=8.3",
        "ext-amqp": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "adam-paterson/oauth2-slack": "1.1.*",
        "api-platform/core": "3.2.*",
        "beberlei/doctrineextensions": "1.3.*",
        "doctrine/annotations": "2.0.*",
        "doctrine/doctrine-bundle": "2.11.*",
        "doctrine/doctrine-migrations-bundle": "3.3.*",
        "doctrine/orm": "2.17.*",
        "embed/embed": "4.4.*",
        "fakerphp/faker": "1.23.*",
        "gedmo/doctrine-extensions": "3.14.*",
        "giggsey/libphonenumber-for-php": "8.13.*",
        "imagine/imagine": "1.3.*",
        "knplabs/knp-menu-bundle": "3.3.*",
        "knplabs/knp-paginator-bundle": "6.3.*",
        "league/oauth2-client": "2.7.*",
        "league/oauth2-facebook": "2.2.*",
        "league/oauth2-github": "3.1.*",
        "league/oauth2-google": "4.0.*",
        "league/oauth2-instagram": "3.1.*",
        "league/oauth2-linkedin": "5.1.*",
        "liip/imagine-bundle": "2.12.*",
        "michaelkaefer/oauth2-amazon": "1.0.*",
        "mmo/faker-images": "0.9.*",
        "nelmio/cors-bundle": "2.4.*",
        "omines/oauth2-gitlab": "3.6.*",
        "phpdocumentor/reflection-docblock": "5.3.*",
        "rtheunissen/oauth2-reddit": "2.0.*",
        "spipu/html2pdf": "5.2.*",
        "stevenmaguire/oauth2-bitbucket": "3.0.*",
        "stevenmaguire/oauth2-dropbox": "3.1.*",
        "stevenmaguire/oauth2-paypal": "2.0.*",
        "symfony/apache-pack": "1.0.*",
        "symfony/asset": "7.0.*",
        "symfony/cache": "7.0.*",
        "symfony/console": "7.0.*",
        "symfony/deprecation-contracts": "3.4.*",
        "symfony/doctrine-messenger": "7.0.*",
        "symfony/dotenv": "7.0.*",
        "symfony/event-dispatcher": "7.0.*",
        "symfony/expression-language": "7.0.*",
        "symfony/finder": "7.0.*",
        "symfony/flex": "2.4.*",
        "symfony/form": "7.0.*",
        "symfony/framework-bundle": "7.0.*",
        "symfony/http-client": "7.0.*",
        "symfony/intl": "7.0.*",
        "symfony/ldap": "7.0.*",
        "symfony/lock": "7.0.*",
        "symfony/mailer": "7.0.*",
        "symfony/mercure": "0.6.*",
        "symfony/messenger": "7.0.*",
        "symfony/mime": "7.0.*",
        "symfony/monolog-bundle": "3.10.*",
        "symfony/notifier": "7.0.*",
        "symfony/process": "7.0.*",
        "symfony/property-access": "7.0.*",
        "symfony/property-info": "7.0.*",
        "symfony/proxy-manager-bridge": "6.4.*",
        "symfony/rate-limiter": "7.0.*",
        "symfony/runtime": "7.0.*",
        "symfony/security-bundle": "7.0.*",
        "symfony/serializer": "7.0.*",
        "symfony/string": "7.0.*",
        "symfony/translation": "7.0.*",
        "symfony/twig-bundle": "7.0.*",
        "symfony/uid": "7.0.*",
        "symfony/validator": "7.0.*",
        "symfony/web-link": "7.0.*",
        "symfony/webapp-meta": "1.0.*",
        "symfony/webpack-encore-bundle": "2.1.*",
        "symfony/workflow": "7.0.*",
        "symfony/yaml": "7.0.*",
        "twig/cssinliner-extra": "3.8.*",
        "twig/extra-bundle": "3.8.*",
        "twig/intl-extra": "3.8.*",
        "vantezzen/oauth2-pinterest": "2.2.*",
        "vertisan/oauth2-twitch-helix": "2.0.*",
        "webonyx/graphql-php": "15.8.*",
        "wohali/oauth2-discord-new": "1.2.*"
    },
    "require-dev": {
        "behat/behat": "3.14.*",
        "doctrine/doctrine-fixtures-bundle": "3.5.*",
        "friendsofphp/php-cs-fixer": "3.47.*",
        "phpmd/phpmd": "2.15.*",
        "phpstan/phpdoc-parser": "1.25.*",
        "phpstan/phpstan": "1.10.*",
        "phpstan/phpstan-deprecation-rules": "1.1.*",
        "phpstan/phpstan-doctrine": "1.3.*",
        "phpstan/phpstan-mockery": "1.1.*",
        "phpstan/phpstan-phpunit": "1.3.*",
        "phpstan/phpstan-symfony": "1.3.*",
        "phpunit/phpunit": "10.5.*",
        "povils/phpmnd": "3.4.*",
        "rector/rector": "0.19.*",
        "squizlabs/php_codesniffer": "3.8.*",
        "symfony/browser-kit": "7.0.*",
        "symfony/css-selector": "7.0.*",
        "symfony/debug-bundle": "7.0.*",
        "symfony/maker-bundle": "1.52.*",
        "symfony/phpunit-bridge": "7.0.*",
        "symfony/stopwatch": "7.0.*",
        "symfony/web-profiler-bundle": "7.0.*",
        "symplify/phpstan-rules": "^12.4"
    },
    "config": {
      "bin-dir": "bin/",
      "optimize-autoloader": true,
      "preferred-install": {
        "*": "dist"
      },
      "platform": {
        "php": "8.3"
      },
      "sort-packages": true,
      "allow-plugins": {
        "composer/package-versions-deprecated": true,
        "symfony/flex": true,
        "symfony/runtime": true
      }
    },
    "autoload": {
      "psr-4": {
        "Labstag\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Labstag\\Tests\\": "tests/"
      }
    },
    "replace": {
      "paragonie/random_compat": "2.*",
      "symfony/polyfill-ctype": "*",
      "symfony/polyfill-iconv": "*",
      "symfony/polyfill-php72": "*"
    },
    "scripts": {
      "php-cs-fixer": [
        "Composer\\Config::disableProcessTimeout",
        "php-cs-fixer fix src"
      ],
      "php-cs-fixer-dry": [
        "Composer\\Config::disableProcessTimeout",
        "php-cs-fixer fix src --dry-run -v --diff"
      ],
      "behat": [
        "Composer\\Config::disableProcessTimeout",
        "behat --config behat.yaml"
     ],
      "phpcbf": [
        "Composer\\Config::disableProcessTimeout",
        "phpcbf --report=diff --extensions=php --standard=phpcs.xml"
      ],
      "phpcs": [
        "Composer\\Config::disableProcessTimeout",
        "phpcs --report=full --extensions=php src --standard=phpcs.xml"
      ],
      "phpcs:error": [
        "Composer\\Config::disableProcessTimeout",
        "phpcs --report=full --extensions=php --warning-severity=0 --standard=phpcs.xml"
      ],
      "phpcs:warning": [
        "Composer\\Config::disableProcessTimeout",
        "phpcs --report=full --extensions=php --error-severity=0 --standard=phpcs.xml"
      ],
      "phpmd": [
        "Composer\\Config::disableProcessTimeout",
        "phpmd src,features/bootstrap ansi phpmd.xml"
      ],
      "phpmnd": [
        "Composer\\Config::disableProcessTimeout",
        "phpmnd src features/bootstrap tests"
      ],
      "phpstan:0": [
        "Composer\\Config::disableProcessTimeout",
        "phpstan analyse -l 0"
      ],
      "phpstan:1": [
        "Composer\\Config::disableProcessTimeout",
        "phpstan analyse -l 1"
      ],
      "phpstan:2": [
        "Composer\\Config::disableProcessTimeout",
        "phpstan analyse -l 2"
      ],
      "phpstan:3": [
        "Composer\\Config::disableProcessTimeout",
        "phpstan analyse -l 3"
      ],
      "phpstan:4": [
        "Composer\\Config::disableProcessTimeout",
        "phpstan analyse -l 4"
      ],
      "phpstan:5": [
        "Composer\\Config::disableProcessTimeout",
        "phpstan analyse -l 5"
      ],
      "phpstan:6": [
        "Composer\\Config::disableProcessTimeout",
        "phpstan analyse -l 6"
      ],
      "phpstan:7": [
        "Composer\\Config::disableProcessTimeout",
        "phpstan analyse -l 7"
      ],
      "phpstan:8": [
        "Composer\\Config::disableProcessTimeout",
        "phpstan analyse -l 8"
      ],
      "phpstan:9": [
        "Composer\\Config::disableProcessTimeout",
        "phpstan analyse -l 9"
      ],
      "rector": [
        "Composer\\Config::disableProcessTimeout",
        "rector process --clear-cache"
      ],
      "rector-dry": [
        "Composer\\Config::disableProcessTimeout",
        "rector process --clear-cache --dry-run"
      ],
      "phpunit": [
        "Composer\\Config::disableProcessTimeout",
        "phpunit"
      ],
      "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
      "post-install-cmd": [
        "@auto-scripts"
      ],
      "post-update-cmd": [
        "@auto-scripts"
      ]
    },
    "conflict": {
      "symfony/symfony": "*"
    },
    "extra": {
      "symfony": {
            "allow-contrib": true,
            "require": "7.0.*",
            "docker": false
        }
    }
  }
