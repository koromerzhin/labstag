{
    "type": "project",
    "name": "koromerzhin/labstag",
    "description": "Site Internet sous Symfony",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-amqp": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "adam-paterson/oauth2-slack": "1.1.3",
        "api-platform/core": "2.6.8",
        "beberlei/doctrineextensions": "1.3.0",
        "bluemmb/faker-picsum-photos-provider": "2.0",
        "doctrine/annotations": "1.13.2",
        "doctrine/doctrine-bundle": "2.5.5",
        "doctrine/doctrine-migrations-bundle": "3.2.2",
        "doctrine/orm": "2.11.1",
        "fakerphp/faker": "v1.18.0",
        "gedmo/doctrine-extensions": "3.5.0",
        "giggsey/libphonenumber-for-php": "8.12.48",
        "helios-ag/fm-elfinder-bundle": "12.1.3",
        "imagine/imagine": "1.2.4",
        "knplabs/knp-menu-bundle": "3.2.0",
        "knplabs/knp-paginator-bundle": "5.8.0",
        "league/oauth2-client": "2.6.1",
        "league/oauth2-facebook": "2.0.5",
        "league/oauth2-github": "3.0.0",
        "league/oauth2-google": "4.0.0",
        "league/oauth2-instagram": "3.0.0",
        "league/oauth2-linkedin": "5.1.2",
        "liip/imagine-bundle": "2.7.6",
        "michaelkaefer/oauth2-amazon": "1.0.3",
        "omines/oauth2-gitlab": "3.4.0",
        "phpdocumentor/reflection-docblock": "5.3.0",
        "phpstan/phpdoc-parser": "1.2.0",
        "rtheunissen/oauth2-reddit": "2.0.2",
        "sensio/framework-extra-bundle": "6.2.6",
        "spipu/html2pdf": "5.2.5",
        "stevenmaguire/oauth2-bitbucket": "3.0.0",
        "stevenmaguire/oauth2-dropbox": "3.1.0",
        "stevenmaguire/oauth2-paypal": "2.0.0",
        "symfony/apache-pack": "1.0.1",
        "symfony/asset": "6.0.7",
        "symfony/cache": "6.0.8",
        "symfony/console": "6.0.8",
        "symfony/doctrine-messenger": "6.0.8",
        "symfony/dotenv": "6.0.5",
        "symfony/event-dispatcher": "6.0.3",
        "symfony/expression-language": "6.0.8",
        "symfony/finder": "6.0.8",
        "symfony/flex": "2.1.8",
        "symfony/form": "6.0.8",
        "symfony/framework-bundle": "6.0.8",
        "symfony/http-client": "6.0.8",
        "symfony/intl": "6.0.8",
        "symfony/ldap": "6.0.8",
        "symfony/lock": "6.0.7",
        "symfony/mailer": "6.0.8",
        "symfony/mercure": "0.6.1",
        "symfony/messenger": "6.0.8",
        "symfony/mime": "6.0.8",
        "symfony/monolog-bundle": "3.7.1",
        "symfony/notifier": "6.0.8",
        "symfony/process": "6.0.8",
        "symfony/property-access": "6.0.8",
        "symfony/property-info": "6.0.7",
        "symfony/proxy-manager-bridge": "6.0.6",
        "symfony/rate-limiter": "6.0.8",
        "symfony/runtime": "6.0.8",
        "symfony/security-bundle": "6.0.8",
        "symfony/serializer": "6.0.8",
        "symfony/string": "6.0.8",
        "symfony/translation": "6.0.8",
        "symfony/twig-bundle": "6.0.8",
        "symfony/uid": "6.0.3",
        "symfony/validator": "6.0.8",
        "symfony/web-link": "6.0.3",
        "symfony/webapp-meta": "1.0.0",
        "symfony/webpack-encore-bundle": "1.14.1",
        "symfony/workflow": "6.0.8",
        "symfony/yaml": "6.0.3",
        "twig/cssinliner-extra": "3.3.5",
        "twig/extra-bundle": "3.3.8",
        "twig/intl-extra": "3.3.5",
        "vantezzen/oauth2-pinterest": "2.2.1",
        "vertisan/oauth2-twitch-helix": "1.1.2",
        "webonyx/graphql-php": "14.11.6",
        "wohali/oauth2-discord-new": "1.1.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "3.4.2",
        "phpunit/phpunit": "9.5.20",
        "rector/rector": "0.12.23",
        "symfony/browser-kit": "6.0.3",
        "symfony/css-selector": "6.0.3",
        "symfony/debug-bundle": "6.0.3",
        "symfony/maker-bundle": "1.41.0",
        "symfony/phpunit-bridge": "6.0.8",
        "symfony/stopwatch": "6.0.5",
        "symfony/web-profiler-bundle": "6.0.8"
    },
    "config": {
      "bin-dir": "bin/",
      "optimize-autoloader": true,
      "preferred-install": {
        "*": "dist"
      },
      "platform": {
        "php": "8.1.9"
      },
      "sort-packages": true,
      "allow-plugins": {
        "composer/package-versions-deprecated": true,
        "symfony/flex": true,
        "symfony/runtime": true
      }
    },
    "autoload": {
      "psr-4": {
        "Labstag\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Labstag\\Tests\\": "tests/"
      }
    },
    "replace": {
      "paragonie/random_compat": "2.*",
      "symfony/polyfill-ctype": "*",
      "symfony/polyfill-iconv": "*",
      "symfony/polyfill-php72": "*"
    },
    "scripts": {
      "phpmnd": "phpmnd src features/bootstrap tests",
      "simple-phpunit": "simple-phpunit --verbose",
      "simple-phpunit-unit-integration": "simple-phpunit --testsuite unit,integration --verbose",
      "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "elfinder:install": "symfony-cmd"
        },
      "post-install-cmd": [
        "@auto-scripts"
      ],
      "post-update-cmd": [
        "@auto-scripts"
      ]
    },
    "conflict": {
      "symfony/symfony": "*"
    },
    "extra": {
      "symfony": {
            "allow-contrib": "true",
            "require": "6.0.*",
            "docker": true
        }
    }
  }
